   <package name="/app/door" extends="default" namespace="/app/door">
 
		<!-- 功能：门锁监控 		作者：QiYanQiang  		时间：2013-04-01-->
		 <action name="init" class="myDoorAction" method="init">
			<result name="init">/app/test/myDoor.jsp</result>		
		 </action>
		 
		  <action name="controlDoor" class="myDoorAction" method="controlDoor">
		      <result name="success">/app/test/myDoor.jsp </result>
		 </action>
       

